        code : |1+
         extern console
         
         
         // Update customer data
         jsNotification is JSON
         jsNotification.event	= "register"
         jsNotification.data		= EDT_Your_name
         
         // Register
         SocketWrite("client", jsNotification)
         
         
         // Clears the looper
         LOOP_CLIENTS.DeleteAll()
         
         // Clears the list of messages
         LOOP_Messages.DeleteAll()
         
         // Adds the first client that logs in
         let N = LOOP_CLIENTS.AddLine()
         LOOP_CLIENTS[ N ].ATT_ID		= JSClient.id
         LOOP_CLIENTS[ N ].ATT_NAME		= EDT_Your_name
         LOOP_CLIENTS[ N ].ATT_WRITE		= False
         
         // Goes to the discussion plane
         MyWindow.Plane					= 3
         
         // Request the list of clients
         jsQuery is JSON
         jsQuery.event = "member_list"
         
         SocketWrite("client", jsQuery)
         
         
