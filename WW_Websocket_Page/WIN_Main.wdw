   -
     internal_properties : CAAAAAgAAAA5niP8DUEpWlsMNf7p/cItHLs1ctXnv0Oh3GrpwODtm3hZDdqZFlBP+GkCRgtSy1pZ1or7LZQ4i++PWU9QLbdzjiAPaVkQIHMP0w7K5lfcpIuMPEQ3bw3wUbxIt1Gg/bgLIr8NqPvb8B3GRpZaoUJfj6rCKS/rTX6KZT97y3i3EdDPVR0Sc51FSuRRIAPbWgIY0g3LUFzvO8z8abZSE7Ocobq6xKpMeF5/0Bg=
     code : |1+
      procedure MyWindow()
      
      MyWindow.Plane=1
      
      PingTimerID	is int
      JSClient	is JSON
      
      // Opens the WebSocket connection
      WebSocketClientConnect("client",SocketConnect_Callback,"localhost",80,"/WW_WebSocketServer")
      
      	INTERNAL PROCEDURE SocketConnect_Callback( nEvent int, sMessage string )
      		
      		switch nEvent
      		
      			// Open the connection
      			case SocketOpening 
      				
      				STC_INFO	= ""
      				
      				// Ping at regular intervals
      				PingTimerID	= Timer(Timer_Callback, 5s)
      					INTERNAL PROCEDURE Timer_Callback()
      						
      						jsPing is JSON
      						jsPing.event = "ping"
      						
      						// Transmission
      						SocketWrite("client", jsPing)
      					end
      				
      			// Receiving a message
      			case SocketMessage
      				
      				jsMessage is JSON = sMessage			
      				
      				switch jsMessage.event
      				
      					// The server validated the connection
      					case "connection_established"
      						
      						// Stores the information returned by the server
      						// to be updated
      						JSClient		= jsMessage.data
      						
      						// Allows users to authenticate
      						MyWindow.Plane	= 2 
      						
      					// Error managed by the server
      					case "bad_request"
      						
      					// Response to <ping>
      					case "pong"
      						
      						// The connection is valid
      						
      					// Another client writes
      					case "typing"
      						
      						// Searches for the client
      						let X = LooperSearch(ATT_ID, jsMessage.data.id)
      						if X > 0 then
      							// Indicates that the client writes
      							ATT_WRITE[ X ] = True
      						end
      						
      					// The list of clients was received
      					case "member_list"
      						
      						// Enumerates cliients
      						for each client of jsMessage.data
      							
      							// Ignores current client
      							if client.id = JSClient.id then continue
      							
      							// Adds client to the list
      							let X = LOOP_CLIENTS.AddLine()
      							
      							LOOP_CLIENTS[ X ].ATT_ID		= client.id
      							LOOP_CLIENTS[ X ].ATT_NAME		= client.nom
      							LOOP_CLIENTS[ X ].ATT_WRITE		= False						
      						end
      						
      						
      					// Another client entered the chat
      					case "member_added"
      						
      						// Ignores current client
      						if jsMessage.data.id = JSClient.id then break switch
      						
      						// Adds client to the list
      						let X = LOOP_CLIENTS.AddLine()
      						LOOP_CLIENTS[ X ].ATT_ID		= jsMessage.data.id
      						LOOP_CLIENTS[ X ].ATT_NAME		= jsMessage.data.nom
      						LOOP_CLIENTS[ X ].ATT_WRITE		= False
      						
      					// Another client left the chat
      					case "member_removed"
      						
      						// Searches for the client
      						let X = LooperSearch(ATT_ID, jsMessage.data.id)
      						if X > 0 then
      							// Deletes the customer
      							LOOP_CLIENTS.Delete(X)
      						end
      						
      					// Another client sent a message
      					case "received"
      						
      						let X = LooperSearch(ATT_ID, jsMessage.data.from.id)
      						if X > 0 then
      							// Indicates that the client is no longer writing
      							ATT_WRITE[ X ] = False
      						end
      						
      						// Adds message to the list
      						let Y = LOOP_Messages.AddLine()
      						LOOP_MESSAGES[ Y ].ATT_WHO		= jsMessage.data.from.nom
      						LOOP_MESSAGES[ Y ].ATT_Message	= jsMessage.data.text
      						
      					// The server disconnects
      					case "server_shutdown"
      						
      						// Close the socket
      						SocketClose("client")					
      						
      				end
      				
      			// Error 
      			case SocketError			
      				
      				// The socket will be closed. Complete in SocketClosing
      				STC_INFO		= "An error occurred" + CR + sMessage
      				
      			// Close 
      			case SocketClosing
      				
      				STC_INFO		+= [ CR ] + "The connection to the server has been interrupted ([%sMessage%])"
      				MyWindow.Plane	= 1
      				
      				EndTimer(PingTimerID)
      				PingTimerID = 0
      				
      		end
      		
      	end 
      
